#!/c/Users/Vincent/scooter/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'nanoleafapi==2.1.1','console_scripts','effects-builder'
__requires__ = 'nanoleafapi==2.1.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('nanoleafapi==2.1.1', 'console_scripts', 'effects-builder')()
    )
